{
    "family": "my-golang-app",
    "networkMode": "bridge",
    "containerDefinitions": [
      {
        "name": "my-golang-app",
        "image": "365309254644.dkr.ecr.eu-central-1.amazonaws.com/my-golang-app:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080
          }
        ],
        "memory": 512,
        "cpu": 256
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ]
  }